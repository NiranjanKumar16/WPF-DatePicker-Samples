<Window x:Class="SfDatePicker_ArrowKeySample.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SfDatePicker_ArrowKeySample"
        mc:Ignorable="d" xmlns:sf="http://schemas.syncfusion.com/wpf"
        Title="SfDatePicker Sample" 
        WindowStartupLocation="CenterScreen"
        Height="350" Width="525">
    <Grid>
        <sf:SfDatePicker
    x:Name="MonthDatePicker"
    VerticalAlignment="Center"
    HorizontalAlignment="Center"
    AllowNull="False" 
    SetValueOnLostFocus="True"
    />

    </Grid>
</Window>
